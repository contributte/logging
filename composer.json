{
  "name": "contributte/logging",
  "description": "Plug-in support logging for Tracy / Nette Framework",
  "keywords": ["nette", "logging", "tracy", "monolog", "plugins"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/logging",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": "^7.2",
    "tracy/tracy": "~2.5.5|~2.6.2|~2.7.0"
  },
  "require-dev": {
    "ext-json": "*",
    "ninjify/qa": "^0.10.0",
    "ninjify/nunjuck": "^0.3.0",
    "nette/di": "^3.0.0",
    "sentry/sentry": "^2.2.2",
    "guzzlehttp/guzzle": "^6.3.3",
    "php-http/guzzle6-adapter": "^2.0.1",
    "phpstan/phpstan-deprecation-rules": "^0.11",
    "phpstan/phpstan-nette": "^0.11",
    "phpstan/phpstan-shim": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11"
  },
  "conflict": {
    "nette/di": "<3.0"
  },
  "suggest": {
    "nette/di": "to use TracyLoggingExtension",
    "sentry/sentry": "to use SentryLoggingExtension"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Logging\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Helpers\\": "tests/helpers"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "0.5.x-dev"
    }
  }
}
